{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sinapsi": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/theme"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "src/theme/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce/skins/lightgray",
                "output": "/assets/tinymce/skin"
              },
              {
                "glob": "theme.min.js",
                "input": "node_modules/tinymce/themes/modern",
                "output": "/assets/tinymce"
              },
              {
                "glob": "plugin.min.js",
                "input": "node_modules/tinymce/plugins/image",
                "output": "/assets/tinymce/plugins/image"
              },
              {
                "glob": "plugin.min.js",
                "input": "node_modules/tinymce/plugins/link",
                "output": "/assets/tinymce/plugins/link"
              },
              {
                "glob": "plugin.min.js",
                "input": "node_modules/tinymce/plugins/lists",
                "output": "/assets/tinymce/plugins/lists"
              }
            ],
            "styles": [
              "src/theme/main.scss"
            ],
            "scripts": [
              "node_modules/moment/min/moment.min.js",
              "node_modules/tinymce/tinymce.min.js",
              "node_modules/codemirror/lib/codemirror.js",
              "node_modules/codemirror/mode/htmlmixed/htmlmixed.js"
            ],
            "serviceWorker": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.en.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "ca": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ca.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "es": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.es.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "en": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.en.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sinapsi:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sinapsi:build:prod"
            },
            "ca": {
              "browserTarget": "sinapsi:build:ca"
            },
            "es": {
              "browserTarget": "sinapsi:build:es"
            },
            "en": {
              "browserTarget": "sinapsi:build:en"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sinapsi:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/moment/min/moment.min.js",
              "node_modules/tinymce/tinymce.min.js",
              "node_modules/codemirror/lib/codemirror.js",
              "node_modules/codemirror/mode/htmlmixed/htmlmixed.js"
            ],
            "styles": [
              "src/theme/main.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/theme/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce/skins/lightgray",
                "output": "/assets/tinymce/skin"
              },
              {
                "glob": "theme.min.js",
                "input": "node_modules/tinymce/themes/modern",
                "output": "/assets/tinymce"
              },
              {
                "glob": "plugin.min.js",
                "input": "node_modules/tinymce/plugins/image",
                "output": "/assets/tinymce/plugins/image"
              },
              {
                "glob": "plugin.min.js",
                "input": "node_modules/tinymce/plugins/link",
                "output": "/assets/tinymce/plugins/link"
              },
              {
                "glob": "plugin.min.js",
                "input": "node_modules/tinymce/plugins/lists",
                "output": "/assets/tinymce/plugins/lists"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "sinapsi-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "sinapsi:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "embed": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/embed",
            "index": "src/embed/index.html",
            "main": "src/embed/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/theme"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "src/theme/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "src/theme/main.scss"
            ],
            "scripts": [
              "node_modules/moment/min/moment.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.en.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "ca": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ca.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "es": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.es.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "en": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.en.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "embed:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "embed:build:prod"
            },
            "ca": {
              "browserTarget": "embed:build:ca"
            },
            "es": {
              "browserTarget": "embed:build:es"
            },
            "en": {
              "browserTarget": "embed:build:en"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "embed:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/moment/min/moment.min.js"
            ],
            "styles": [
              "src/theme/main.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/theme/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "embed-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "embed:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "sinapsi",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}